<!DOCTYPE BankData[
	<!ELEMENT Customer (Address, Phone)>
		<!ATTLIST Customer
			name CDATA #REQUIRED
			SSNo ID	   #REQUIRED
			AcountOwned IDREFS  #REQUIRED
		>
	<!ELEMENT Address (Street, City)>
	<!ElEMENT Street (#PCDATA)>
	<!ELEMENT City (#PCDATA)>
	<!ELEMENT Phone (#PCDATA)>
	
	<!ELEMENT BankAcount (Balance)>
		<!ATTLIST BankAcount
			Numbers	ID #REQUIRED
			Type	(savings|checking) #REQUIRED
			Owner	IDREFS	#REQUIRED
		>
	<!ELEMENT Balance (#PCDATA)>
]>


